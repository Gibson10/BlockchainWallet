<!DOCTYPE html>
<html> 
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta http-equiv="x-ua-compatible" content="ie=edge">
        <meta property="og:image" content="/media/abi_logo.png" />

        <title>AEON</title>

        <!-- Bootstrap core CSS -->
        <link href="/css/bootstrap.min.css" rel="stylesheet">

        <!-- Material Design Bootstrap -->
        <link href="/css/mdb.min.css" rel="stylesheet">
        <link  href="/css/owl.carousel.min.css" rel="stylesheet" >
        <link rel="icon" href="/media/fabicon.png" rel="stylesheet" >
        <link href="/css/animate.min.css">
          <link rel="stylesheet" href="/css/font-awesome.min.css">
          <link rel="stylesheet" href="/css/slicknav.min.css">
          <link rel="stylesheet" href="/css/animation.css">

          <link rel="stylesheet" href="/css/material-design-iconic-font.min.css">
          <link rel="stylesheet" href="/css/responsive.css">
        <!-- Datatables -->
        <link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-table/1.11.0/bootstrap-table.min.css" rel="stylesheet">

        <!-- Custom styles -->
        <link href="/css/style2.css" rel="stylesheet">
        
        <!-- Font Awesome -->
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
        
        <!-- Pagination -->
        <link href="https://cdnjs.cloudflare.com/ajax/libs/paginationjs/2.1.2/pagination.css" rel="stylesheet">
        
        <!-- Favicon -->
        <link rel="shortcut icon" href="/media/favicon.ico" type="image/x-icon">
        <link rel="icon" href="/media/favicon.ico" type="image/x-icon">

        <script src="/js/jquery-2.2.4.min.js"></script>
        <script src="/js/popper.js"></script>
        <script src="/js/owl.carousel.min.js"></script>
        <script src="/js/wow.min.js"></script>

        <script src="/js/bootstrap.min.js"></script>
        <!--skroller js-->
        <script src="/js/skrollr.min.js"></script>
        <!--mobile menu js-->
        <script src="/js/jquery.slicknav.min.js"></script>
        <!--particle s-->
        <script src="/js/particles.min.js"></script>
        <!-- main js-->
        <script src="/js/main.js"></script>

    

    </head>

    <body>

        <div class="row container-fluid">
        <div class="col-md-2"></div>
            <div class="col-md-8">
                <% if(success && success.length > 0){ %>
                    <div class="my-5 alert alert-success alert-dismissible fade show" role="alert">
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                        </button>
                        <strong>Woo Hoo!</strong> <%= success %>
                    </div>
                <% } %>
                <% if(error && error.length > 0){ %>
                    <div class="my-5 alert alert-danger alert-dismissible fade show" role="alert">
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                        </button>
                        <strong>Bummer!</strong> <%= error %>
                    </div>
                <% } %>
            </div>
            <div class="col-md-2"></div>
        </div>



        